package com.newlecture.code.func;

public class App2 {
	// 덧셈 출력
	static int Add() {
		int x = 3;
		int y = 4;

		return x + y;
	}

//	------------------------------------------
	// 뺄셈 출력
	{
		int x = 3;
		int y = 4;

		int result = x - y;
		System.out.println(result);
	}

	public static void main(String[] args) {
		{
			int result = Add();
			System.out.println(result);
		}
	}

}
