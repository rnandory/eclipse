package com.newlecture.code.func;

public class App2 {
	// 덧셈 출력
	static int add() {
		int x = 3;
		int y = 4;

		return x + y;
	}

	static void print() {
		System.out.println(add());
	}

//	------------------------------------------
	// 뺄셈 출력
	static int sub() {
		int x = 3;
		int y = 4;

		return x - y;
	}

	public static void main(String[] args) {
		// 덧셈출력
		{
			int result = add();
			System.out.println(result);
			print();
		}
	}

}
