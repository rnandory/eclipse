package com.newlecture.code.func;

public class App {

	static void printAdd() {
		int x = 3;
		int y = 4;

		int result = x + y;
		System.out.println(result);
	}

	/*
	 * public static int add(int x, int y) { return x + y; }
	 */

	public static void main(String[] args) {
		/*
		 * int z = add(3, 4); System.out.println(add(5, 6));
		 */
		printAdd();
	}
}
